{
  "name": "xfactor-viral-growth",
  "version": "1.0.0",
  "description": "10x K Factor Viral Growth System for Varsity Tutors",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "server": "tsx server/index.ts",
    "db:init": "tsx database/init.ts",
    "db:seed": "tsx database/seed.ts",
    "db:seed-cohort": "tsx database/seed-cohort.ts",
    "db:reset": "rm -f xfactor.db && npm run db:init && npm run db:seed",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "vitest run tests/e2e",
    "test:acceptance": "vitest run tests/acceptance",
    "test:security": "vitest run tests/security",
    "test:performance": "vitest run tests/performance",
    "test:all": "vitest run",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "viral-growth",
    "k-factor",
    "varsity-tutors",
    "mcp",
    "agent-based"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "tsx": "^4.20.6"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^9.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.1"
  }
}
